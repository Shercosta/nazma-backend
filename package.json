{
  "name": "todos-express-starter",
  "version": "0.0.0",
  "private": true,
  "description": "Starter todo app using Express, Passport, and SQLite for sign in.",
  "keywords": [
    "example",
    "express",
    "passport",
    "sqlite"
  ],
  "author": {
    "name": "Jared Hanson",
    "email": "jaredhanson@gmail.com",
    "url": "https://www.jaredhanson.me/"
  },
  "homepage": "https://github.com/passport/todos-express-starter",
  "repository": {
    "type": "git",
    "url": "git://github.com/passport/todos-express-starter.git"
  },
  "bugs": {
    "url": "https://github.com/passport/todos-express-starter/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/jaredhanson"
  },
  "license": "Unlicense",
  "scripts": {
    "start": "node ./bin/www",
    "migrate": "sequelize-cli db:migrate",
    "migrate:create": "sequelize-cli migration:generate --name",
    "migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "migrate:undo": "sequelize-cli db:migrate:undo --name",
    "seed": "sequelize-cli db:seed:all --debug",
    "seed:create": "sequelize-cli seed:generate --name",
    "seed:undo:all": "sequelize-cli db:seed:undo:all",
    "seed:undo": "sequelize-cli db:seed:undo --seed",
    "model:generate": "sequelize-cli model:generate --name"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "connect-sqlite3": "^0.9.13",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.6.0",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "express-mysql-session": "^2.1.8",
    "express-session": "^1.17.3",
    "http-errors": "~1.6.3",
    "lodash": "^4.17.21",
    "mkdirp": "^1.0.4",
    "moment": "^2.29.4",
    "morgan": "~1.9.1",
    "mysql2": "^2.3.3",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pluralize": "^8.0.0",
    "sequelize": "^6.27.0",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/lodash": "^4.14.191",
    "@types/moment": "^2.13.0",
    "sequelize-cli": "^6.5.2"
  }
}
